		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-node-globals": "^1.4.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-size-snapshot": "^0.11.0",
		"rollup-plugin-terser": "^5.3.0"
	},
	"resolutions": {
		"**/@babel/core": "^7.7.5",
		"**/@babel/plugin-proposal-class-properties": "^7.7.5",
		"**/@babel/plugin-proposal-object-rest-spread": "^7.7.5",
		"**/@babel/plugin-transform-runtime": "^7.7.5",
		"**/@babel/preset-env": "^7.7.4",
		"**/@babel/preset-react": "^7.7.5",
		"**/@babel/runtime-corejs2": "^7.7.5",
		"**/@babel/runtime": "^7.7.5",
		"**/next/terser": "^4.1.2",
		"**/@emotion/core": "^10.0.28"
	},
	"scripts": {
		"build": "yarn build:node-packages && yarn examples build",
		"start": "concurrently -k -n \"NODE,APP\" \"yarn start:node-packages\" \"yarn examples start\"",
		"build:node-packages": "lerna run build --ignore @fast-ai/docs-examples --ignore @fast-ai/storybook",
		"start:node-packages": "lerna run start --parallel --ignore @fast-ai/docs-examples --ignore @fast-ai/storybook",
		"lint": "yarn lint:pkg && yarn lint:js",
		"lint:pkg": "npmPkgJsonLint .",
		"lint:js": "eslint --cache --ignore-path .gitignore --ext .js ./",
		"prepare": "yarn build:node-packages",
		"prepublish": "yarn build:node-packages",
		"prepublishOnly": "yarn build:node-packages",
		"test": "jest",
		"format": "prettier --ignore-path .gitignore --write '**/*.{js,json}' && yarn lint:js --fix",
		"generate": "plop --plopfile \"./.plop/plopfile.js\"",
		"storybook": "yarn workspace @fast-ai/storybook",
		"examples": "yarn workspace @fast-ai/docs-examples",
		"ui-components": "yarn workspace @fast-ai/ui-components",
		"gatsby-theme-fast-ai": "yarn workspace gatsby-theme-fast-ai",
		"gatsby-plugin-setup": "yarn workspace @fast-ai/gatsby-plugin-setup"
	},
	"workspaces": [
		"packages/*",
		"apps/*"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "yarn lint && yarn test"
		}
	}
}
