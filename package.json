		"lint-staged": "^10.1.2",
		"nodemon": "^2.0.2",
		"npm-package-json-lint": "^4.5.0",
		"npm-package-json-lint-config-default": "^2.0.0",
		"plop": "^2.6.0",
		"prettier": "^2.0.4",
		"prop-types": "^15.7.2",
